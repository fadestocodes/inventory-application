<body>
    <%- include('partials/navbar') %>
    <h1>Add To Inventory</h1>
    <a href="/add/product">Add a New Product</a>
    <a href="/add/category">Add a New Category</a>
    <h2>Current Products</h2>
    <div>
        <% for (item of allProducts){ %>
            <p><%=item.name%></p>
        <%}%> 
    </div>
    <h2>Current Categories</h2>
    <div>
        <% for (element of allCategories){ %>
            <p><%=element.name%></p>
        <%}%>
    </div>
</body>